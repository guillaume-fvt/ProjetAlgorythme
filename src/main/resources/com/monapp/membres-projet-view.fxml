<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.monapp.controller.MembresProjetController"
            prefHeight="300.0" prefWidth="400.0">

    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Liste du Personnel" />

        <ListView fx:id="listViewEmployes" layoutX="14.0" layoutY="40.0"
                  prefHeight="200.0" prefWidth="200.0" />

        <Button fx:id="btnAjouterMembre" layoutX="240.0" layoutY="40.0"
                text="Ajouter Membre" onAction="#ajouterMembreAuProjet"/>
        <Button fx:id="btnFermer" layoutX="240.0" layoutY="90.0"
                text="Fermer" onAction="#fermerFenetre"/>
    </children>
</AnchorPane>
